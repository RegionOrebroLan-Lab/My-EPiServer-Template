<!DOCTYPE html>
<html lang="@(ViewData["Culture"] ?? "sv")">
	<head>
		@if(IsSectionDefined("HeadContent"))
		{
			@RenderSection("HeadContent")
		}
		else
		{
			if(IsSectionDefined("HeadMeta"))
			{
				@RenderSection("HeadMeta")
			}
			else
			{
				<partial name="~/Views/Shared/Parts/Head/Meta.cshtml" />
			}
			if(IsSectionDefined("HeadTitle"))
			{
				@RenderSection("HeadTitle")
			}
			else
			{
				<partial name="~/Views/Shared/Parts/Head/Title.cshtml" />
			}
			if(IsSectionDefined("HeadLink"))
			{
				@RenderSection("HeadLink")
			}
			else
			{
				<partial name="~/Views/Shared/Parts/Head/Link.cshtml" />
			}
			if(IsSectionDefined("HeadScript"))
			{
				@RenderSection("HeadScript")
			}
			else
			{
				<partial name="~/Views/Shared/Parts/Head/Script.cshtml" />
			}
			if(IsSectionDefined("AdditionalHeadContent"))
			{
				@RenderSection("AdditionalHeadContent")
			}
		}
	</head>
	<body>
		<a accesskey="2" class="accessibility-first" href="#main">
			Hoppa till huvudinnehållet
			<svg class="icon -arrow-right" role="presentation">
				<use xlink:href="/Style/Icons/sprite.svg#arrow-right" />
			</svg>
		</a>
		@if(IsSectionDefined("Header"))
		{
			@RenderSection("Header")
		}
		else
		{
			<partial name="~/Views/Shared/Parts/Header.cshtml" />
		}
		@RenderBody()
		@if(IsSectionDefined("Footer"))
		{
			@RenderSection("Footer")
		}
		else
		{
			<partial name="~/Views/Shared/Parts/Footer.cshtml" />
		}
		<partial name="~/Views/Shared/Parts/Scripts.cshtml" />
		@if(IsSectionDefined("AdditionalScripts"))
		{
			@RenderSection("AdditionalScripts")
		}
	</body>
</html>